@model TWO_POKEMON_TORRALBA_LAIZA_BSIT_32E1.Models.Pokemon

<h2>@Model.Name</h2>

<h3>Abilities</h3>
<ul>
    @foreach (var ability in Model.Abilities)
    {
        <li>@ability</li>
    }
</ul>

<h3>Moves</h3>
<ul>
    @foreach (var move in Model.Moves)
    {
        <li>@move</li>
    }
</ul>

<a href="@Url.Action("Index", "Pokemon")">Back to List</a>